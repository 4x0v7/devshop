{
    "name": "devshop/cli",
    "description": "Command Line interface for DevShop",
    "require": {
        "symfony/console": "^2.7",
        "symfony/process": "^2.7",
        "knplabs/github-api": "^1.4",
        "symfony/filesystem": "^2.7",
        "herrera-io/phar-update": "1.*",
        "cpliakas/git-wrapper": "1.*",
        "asm/php-ansible": "^1.0",
        "consolidation/robo": "^1.3",
        "drush/drush": "~8"
    },
    "authors": [
        {
            "name": "Jon Pugh",
            "email": "jon@thinkdrop.net"
        }
    ],
    "autoload": {
        "psr-0": { "DevShop": "src/" }
    },
    "bin": [
        "devshop"
    ],
    "config": {
        "platform": {
            "php": "5.5.38"
        },
        "bin-dir": "bin"
    }
}
